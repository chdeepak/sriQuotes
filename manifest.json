{
  "manifest_version": 2,

  "name": "Sri Sri Quotes",
  "description": "This extension shows a Quotes When you open a new tab",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
     "default_title": "Click here!"
  },
  "permissions": [
    "tabs",
    "history",
    "topSites",
    "browsingData"
  ],
  "chrome_url_overrides" : {
    /*"newtab": "newpage.html",*/
    "newtab": "menupage.html"
  }
}
